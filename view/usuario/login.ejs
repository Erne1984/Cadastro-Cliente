<html>
    <body>
        <h1>Cadastro Usuario</h1>
        <form action="/login/salvar" method="post">
            <label> Nome</label>
            <input name="nome" type="text"> <br>
            <label >Senha</label>
            <input name="senha" type="password"> <br>
            <input type="submit" value="entrar">
        </form>

        <% if(mensagem) { %>

            <p> 
                <label style="color: red;"><%= mensagem %></label>
            </p>

        <%}>
    </body>
</html>